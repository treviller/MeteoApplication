{% extends 'MeteoBundle:Meteo:layout.html.twig' %}

{% block title %}
	Home - {{ parent() }}
{% endblock %}

{% block body %}
	<div class="jumbotron">
		<h1>Welcome to this new meteo site !</h1>
	</div>
	
	{% for key, messages in app.session.flashbag.all() %}
        {% for message in messages %}
			<div class="alert alert-{{ key }}">
       	        {{ message }}
      		</div>
        {% endfor %}
    {% endfor %}
{% endblock %}